{
	"compilerOptions": {
		"resolveJsonModule": true,
		"target": "ESNext",
		"module": "CommonJS",
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"importsNotUsedAsValues": "error",
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
